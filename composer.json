{
    "name": "c517718004/drmking-addons",
    "description": "thinkphp8多应用插件",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "常立超",
            "email": "517718004@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\addons\\Service"
            ],
            "config": {
                "addons": "src/config.php"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php think addons:config"
        ]
    }
}